<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.hw7.controller.ConfirmController"
            prefHeight="178.0" prefWidth="342.0">
    <Button onAction="#onDownloadButtonClick" layoutX="200.0" layoutY="135.0" mnemonicParsing="false" text="Скачать">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Label layoutX="42.0" layoutY="29.0"/>
    <Label layoutX="76.0" layoutY="17.0" text="Информация о файле">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Label>
    <Label fx:id="nameLabel" layoutX="76.0" layoutY="49.0" text="Имя">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label fx:id="editDateLabel" layoutX="76.0" layoutY="69.0" text="Дата изменения">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label fx:id="sizeLabel" layoutX="76.0" layoutY="91.0" text="Размер">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Button onAction="#onCancelClick" layoutX="76.0" layoutY="135.0" mnemonicParsing="false" text="Отмета">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>

</AnchorPane>
