<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.hw5.view.ExportLayoutController"
            prefHeight="200.0" prefWidth="400.0">
    <children>
        <Label layoutX="14.0" layoutY="66.0" text="Путь до файла" />
        <Button fx:id="okBtn" layoutX="310.0" layoutY="136.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="62.0" text="Ок" onAction="#handleOkBtn" />
        <TextField fx:id="fileSetField" layoutX="103.0" layoutY="62.0" prefHeight="25.0" prefWidth="267.0" onMouseClicked="#handleFileSetFieldClick" />
        <Button layoutX="224.0" layoutY="136.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Отмена" onAction="#cancelBtnHandle" />
    </children>
</AnchorPane>
